{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "SCU-LOADTRACKINGDATA-ADF-DEV"
		},
		"DM360T_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'DM360T'"
		},
		"MySql1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MySql1'"
		},
		"MySql_360Tracking_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MySql_360Tracking'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "SCU-LOADTRACKINGDATA-ADF-DEV_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"DM360T_connectionString": {
						"value": "[parameters('DM360T_connectionString')]"
					},
					"MySql1_connectionString": {
						"value": "[parameters('MySql1_connectionString')]"
					},
					"MySql_360Tracking_connectionString": {
						"value": "[parameters('MySql_360Tracking_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}